{
  "name": "ts-adt",
  "version": "2.1.2",
  "author": "Paul Gray <pfbgray@gmail.com>",
  "description": "An Algebraic Data Type generator for Typescript",
  "main": "ADT.js",
  "types": "ADT.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc && ts-node ./scripts/build",
    "prepublish": "tsc",
    "test": "c8 --reporter html ava ",
    "prerelease": "yarn build",
    "release": "ts-node scripts/release"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "ava": "^3.13.0",
    "fp-ts": "^2.9.0",
    "glob": "^7.1.6",
    "ts-node": "^9.1.0",
    "tsd": "^0.14.0",
    "typescript": "3.9"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "c8": "^7.11.0"
  }
}
